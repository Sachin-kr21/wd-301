{
    "compilerOptions": {
      "jsx": "react",
      "esModuleInterop": true,
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      // "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "types": [
        "react",
        "react-dom"
      ],
    },
    "include": ["src"]
}
